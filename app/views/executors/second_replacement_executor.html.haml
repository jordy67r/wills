  = f.fields_for :second_replacement_executor_general_detail do |builder|
    = render "general_details/general_details_form", :builder => builder, :nested_origin => @executor.second_replacement_executor_general_detail
  .field
    = question "03080"
    = f.check_box :replacement_third

  = f.submit 'Save', class: "button radius"