= render 'header'
%h4 Second Guardian


= form_for [@will, @guardian] do |f|
  = f.fields_for :second_guardian_general_detail do |builder|
    = render "general_details/general_details_form", :builder => builder, :nested_origin => @guardian.second_guardian_general_detail
  .field
    = question "05040"
    = f.check_box :third_guardian

  = f.submit 'Save', class: "button radius"